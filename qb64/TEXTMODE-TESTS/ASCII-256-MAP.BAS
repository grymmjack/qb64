DIM i AS _UNSIGNED _BYTE
WIDTH 40, 25
CLS
_BLINK OFF
_CONTROLCHR OFF
i = 0
DO
 PRINT CHR$(i);
 i = i + 1
 IF (i AND &HF) = 0 THEN PRINT
LOOP WHILE i
LOCATE 1, 20
DO
 COLOR i AND &HF OR (i AND &H80) \ &H8, (i AND &H70) \ &H10
 PRINT CHR$(i);
 i = i + 1
 IF (i AND &HF) = 0 THEN LOCATE 1 + i \ &H10, 20
LOOP WHILE i
END 
