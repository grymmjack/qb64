$INCLUDEONCE

TYPE TIMER_OBJECT
    STARTED   AS DOUBLE
    STOPPED   AS DOUBLE
    TICKS     AS LONG
    SECONDS   AS LONG
    HOLD      AS INTEGER
    LAP_COUNT AS INTEGER
END TYPE

TYPE TIMER_LAP
    STARTED AS DOUBLE
    STOPPED AS DOUBLE
    TICKS   AS LONG
END TYPE

DIM SHARED MAIN_TIMER AS INTEGER
DIM SHARED TICKS_TIMER AS TIMER_OBJECT
DIM SHARED TIMER_LAPS(10) AS TIMER_LAP
