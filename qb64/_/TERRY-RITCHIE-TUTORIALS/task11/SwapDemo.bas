'--------------------------------
'- Variable Declaration Section -
'--------------------------------

DIM Var1%
DIM Var2%
DIM S1$
DIM S2$

'----------------------------
'- Main Program Begins Here -
'----------------------------

S1$ = "String 1"
S2$ = "String 2"
Var1% = 1
Var2% = 2
PRINT
PRINT " Before swap"
PRINT " -----------"
PRINT
PRINT " Var1% ="; Var1%
PRINT " Var2% ="; Var2%
PRINT " S1$   = "; S1$
PRINT " S2$   = "; S2$
SWAP S1$, S2$
SWAP Var1%, Var2%
PRINT
PRINT " After swap"
PRINT " ----------"
PRINT
PRINT " Var1% ="; Var1%
PRINT " Var2% ="; Var2%
PRINT " S1$   = "; S1$
PRINT " S2$   = "; S2$




























